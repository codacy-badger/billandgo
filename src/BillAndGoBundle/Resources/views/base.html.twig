    <script type="text/javascript">
        window.onload = function () {
            document.getElementsByClassName("se-pre-con")[0].style.display = "none";

        };

        var formatingDate  = function(date) {
            var type = 0;
            if ("undefined" !== typeof date.dateTime) {
                type = 1;
                date = new Date(date.dateTime);
            }
            else {
                date = new Date(date.date);
            }

            var monthNames = [
                "Janvier", "Février", "Mars",
                "Avril", "Mai", "Juin", "Juillet",
                "Août", "Septembre", "Octobre",
                "Novembre", "Décembre"
            ];

            var day = date.getDate();
            var monthIndex = date.getMonth();
            var year = date.getFullYear();

            if (1 === type) {
                var h = date.getHours();
                if (h < 10) {
                    h = '0' + h;
                }
                var i = date.getMinutes();
                if (i < 10) {
                    i = '0' + i;
                }
                var s = date.getSeconds();
                if (s < 10) {
                    s = '0' + s;
                }
                return day + ' ' + monthNames[monthIndex] + ' ' + year+ ' à '+ h+':'+i+':'+s;
            }
            else {
                return day + ' ' + monthNames[monthIndex] + ' ' + year;
            }
        };


        var formatingDate2  = function(date) {
            var type = 0;
            if ("undefined" !== typeof date.dateTime) {
                type = 1;
                date = new Date(date.dateTime);
            }
            else {
                date = new Date(date.date);
            }

            var day = date.getDate();
            if (day < 10) {
                day = '0' + day;
            }
            var monthIndex = date.getMonth() + 1;
            if (monthIndex < 10) {
                monthIndex = '0' + monthIndex;
            }
            var year = date.getFullYear();

            if (1 === type) {
                var h = date.getHours();
                if (h < 10) {
                    h = '0' + h;
                }
                var i = date.getMinutes();
                if (i < 10) {
                    i = '0' + i;
                }
                var s = date.getSeconds();
                if (s < 10) {
                    s = '0' + s;
                }
                //return (date.toUTCString());
                return (year + '-' + (monthIndex) + '-' + day+ 'T'+ h+':'+i+':'+s);
            }
            else {
                //return (date.toUTCString());
                return (year + '-' + (monthIndex) + '-' + day);
            }
        };

    </script>
